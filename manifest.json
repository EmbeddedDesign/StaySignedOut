{
	"name": "__MSG_extName__",
	"short_name": "__MSG_shortName__",
	"version": "0.0.6",
	"author": "operator",
	"default_locale": "en",
	"description": "__MSG_extDesc__",
	"permissions": [
		"tabs"
	],
	"icons": {
		"16": "images/16x16.png",
		"32": "images/32x32.png",
		"48": "images/48x48.png",
		"128": "images/128x128.png"
	},
	"page_action": {
		"default_title": "Stay Signed Out",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["*://accounts.google.com/*"],
			"js": ["uncheck.js"]
		},
		{
			"matches": ["*://staysignedout.com/*"],
			"run_at": "document_end",
			"js": ["installCheck.js"]
		}
	],
	"manifest_version": 2
}